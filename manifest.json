{
  "manifest_version": 2,

  "name": "phiscitello",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.jpg",
    "default_popup": "popup.html"
  },


  "content_scripts" : [
    {
      "matches":["http://*/*","https://*/*"],
      "js":["jquery-2.1.1.js","underscore.js", "backgroundScript.js", "interceptForm.js", "checkPassword.js", "sha256.js"]

    }
  ]
}